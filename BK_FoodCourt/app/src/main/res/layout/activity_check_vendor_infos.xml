<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".TotalRevenueItem_StatReport">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                style="@style/vendorID_name"
                android:text="@string/store_number_1" />

            <ImageButton
                android:id="@+id/vendor1_onclick"
                android:layout_width="match_parent"
                android:layout_height="200dp" />

            <TextView
                style="@style/vendorID_name"
                android:text="@string/store_number_2" />

            <ImageButton
                android:id="@+id/vendor2_onclick"
                android:layout_width="match_parent"
                android:layout_height="200dp" />

            <TextView
                style="@style/vendorID_name"
                android:text="VENDOR NUMBER 3"
                />
            <ImageButton
                android:id="@+id/vendor3_onclick"
                android:layout_width="match_parent"
                android:layout_height="200dp"/>
            <TextView
                style="@style/vendorID_name"
                android:text="VENDOR NUMBER 4"/>
            <ImageButton
                android:id="@+id/vendor4_onclick"
                android:layout_height="200dp"
                android:layout_width="match_parent"/>
            <TextView
                style="@style/vendorID_name"
                android:text="VENDOR NUMBER 5"/>
            <ImageButton
                android:id="@+id/vendor5_onclick"
                android:layout_height="200dp"
                android:layout_width="match_parent"/>

        </LinearLayout>
    </ScrollView>


</LinearLayout>